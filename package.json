{
  "name": "vueginate",
  "version": "1.0.0",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vueginate.umd.cjs",
  "module": "./dist/vueginate.js",
  "style": "./dist/css/vueginate.css",
  "types": "./dist/vueginate.d.ts",
  "exports": {
    ".": {
      "types": "./dist/vueginate.d.ts",
      "import": "./dist/vueginate.js",
      "require": "./dist/vueginate.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite demo --config ./vite.config.ts",
    "build": "run-p type-check:lib build-only",
    "preview": "vite preview",
    "test": "vitest",
    "build-only": "vite build && pnpm tsc-types",
    "tsc-types": "sh build/tsc-types.sh",
    "type-check": "run-s type-check:lib type-check:demo",
    "type-check:lib": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "type-check:demo": "vue-tsc --noEmit -p demo/tsconfig.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write lib/ demo/src/ demo/*.config.js tests/ .eslintrc.cjs *.config.ts"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "tailwindcss": "^3.3.1",
    "typescript": "~4.8.4",
    "vite": "^4.3.1",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.6.4"
  }
}
