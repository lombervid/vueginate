{
  "name": "@vueginate/demo",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-s build:vueginate build:app",
    "build:app": "npm run type-check && vite build",
    "build:vueginate": "npm run --workspace vueginate build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit -composite false"
  },
  "dependencies": {
    "vue": "^3.2.47",
    "vueginate": "workspace:^"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/tsconfig": "^0.1.3",
    "vite": "^4.3.1",
    "vue-tsc": "^1.6.4"
  }
}
